{
  "profiles": {
    "tap (Install Editor CE)": {
      "commandName": "Project",
      "commandLineArgs": "package install \"Editor CE\" --no-dependencies",
      "environmentVariables": {
        "OPENTAP_COLOR": "1"
      }
    },
    "tap (list)": {
      "commandName": "Project",
      "commandLineArgs": "package list -v"
    },
    "tap (create)": {
      "commandName": "Project",
      "commandLineArgs": "package create ..\\..\\opentap.x64.package.xml -v"
    },
    "tap (gitversion)": {
      "commandName": "Project",
      "commandLineArgs": "sdk gitversion --dir c:\\git\\tapMerge\\ --log 20"
    },
    "tap (commands)": {
      "commandName": "Project"
    },
    "tap (run)": {
      "commandName": "Project",
      "commandLineArgs": "run ../../tests/RepeatOutputCompare.TapPlan"
    },
    "tap (profile)": {
      "commandName": "Project",
      "commandLineArgs": "profile --test OpenTap.UnitTests.ResourceTest.EnabledIfResourceWarningTest"
    }
  }
}